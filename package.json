{
  "name": "backend",
  "version": "0.31.10",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn migrate:latest && node dist/src/main",
    "start:local": "yarn migrate:latest && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "node dist/src/main",
    "start:nest": "nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "eslint:report": "eslint -f json -o ./reports/eslint.report.json \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:band": "jest --runInBand",
    "test:report": "jest --verbose --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migrate:latest": "npx knex migrate:latest",
    "version:major": "yarn version --major --no-commit-hooks && git push --no-verify",
    "version:minor": "yarn version --minor --no-commit-hooks && git push --no-verify",
    "version:patch": "yarn version --patch --no-commit-hooks && git push --no-verify"
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "^3.0.0",
    "@google-cloud/pubsub": "^2.19.0",
    "@nestjs/axios": "0.0.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.0.9",
    "@ntegral/nestjs-sendgrid": "^1.0.0",
    "@sendgrid/client": "^7.4.7",
    "@sendgrid/helpers": "^7.4.6",
    "@sendgrid/mail": "^7.4.7",
    "@sentry/integrations": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "firebase-admin": "^9.11.1",
    "firebase-dynamic-links": "^1.1.0",
    "google-auth-library": "^7.9.2",
    "handlebars": "^4.5.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "knex": "^1.0.1",
    "nodemailer": "^6.7.0",
    "pg": "^8.7.1",
    "pg-boss": "^7.1.0",
    "puppeteer": "^15.3.0",
    "puppeteer-cluster": "^0.23.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^10.10.0",
    "swagger-ui-express": "^4.1.6",
    "systeminformation": "^5.9.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.185",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/ramda": "^0.27.46",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "node-mocks-http": "^1.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-node-externals": "^3.0.0"
  },
  "pre-commit": [
    "lint",
    "test:band"
  ],
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test.report.xml",
    "indent": 4
  },
  "resolutions": {
    "ansi-regex": "^4.1.1",
    "axios": "^0.21.2",
    "follow-redirects": "^1.14.7",
    "minimist": "^1.2.6",
    "node-fetch": "^2.6.7",
    "node-forge": "^1.3.0",
    "shelljs": "^0.8.5",
    "swagger-ui-dist": "^4.1.3",
    "validator": "^13.7.0"
  }
}
